                    </li>
                    <li class="nav-item">
                        @*<a href="/Home/Pricing" class="nav-link">Pricing</a>*@
                        @Html.ActionLink(Resources.lang.Menu_Pricing, "Pricing", "Home", new { id = "" }, new { @class = "nav-link" })

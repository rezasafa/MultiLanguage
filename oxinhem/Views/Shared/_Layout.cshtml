<body>
    @{ 
        var db = new oxinhem.Models.MyAppdbContext();
        var Co = new MyModels.Company();
        Co = db.companies.FirstOrDefault();
        var routeValues = this.ViewContext.RouteData.Values;
        var controller = routeValues["controller"] as string;
        var action = routeValues["action"] as string;
    }
    <div class="wrap">
        <div class="container">
            <div class="row justify-content-between">
                <div class="col-12 col-md d-flex align-items-center">
                    <p class="mb-0 phone">
                        <span class="mailus">@Resources.lang.Home_Phone</span>
                        <a href="tel:@Co.Tel1">@Co.Tel1</a>
                        <span class="mailus">@Resources.lang.Home_Email</span>
                        <a href="mailto:@Co.Email">@Co.Email</a>
                        @Html.ActionLink("_", @action, @controller, new { culture = "en" }, new { rel = "alternate", hreflang = "en", @class = "flag-icon flag-icon-us" })
                        @Html.ActionLink("_", @action, @controller, new { culture = "se" }, new { rel = "alternate", hreflang = "se", @class = "flag-icon flag-icon-se" })
                    </p>    
                </div>
                <div class="col-12 col-md d-flex justify-content-md-end">
                    <div class="social-media">
                        <p class="mb-0 d-flex">
                            <a href="@Co.Facebook" class="d-flex align-items-center justify-content-center"><span class="fa fa-facebook"><i class="sr-only">Facebook</i></span></a>
                            <a href="@Co.Instagram" class="d-flex align-items-center justify-content-center"><span class="fa fa-instagram"><i class="sr-only">Instagram</i></span></a>  
                        </p>
                    </div>
                </div>
            </div>
        </div>
    </div>
